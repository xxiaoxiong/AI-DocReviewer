{
  "protocol_id": "软件单元测试记录DC",
  "name": "（软件文档标识）",
  "version": "1.0",
  "description": "从 软件单元测试记录DC.docx 通过LLM提取",
  "categories": [
    {
      "category": "范围",
      "rules": [
        {
          "rule_id": "R001",
          "description": "文档必须包含标识节，给出系统和软件的完整标识，包括标识号、名称、缩略语、版本号及发行号等。",
          "check_type": "structure",
          "keywords": [
            "标识",
            "完整标识",
            "版本号",
            "发行号"
          ],
          "positive_examples": [
            "标识节包含：软件名称XYZ，版本号V1.0，发行号2023-001。"
          ],
          "negative_examples": [
            "标识节仅列出软件名称，缺少版本号和发行号。"
          ],
          "severity": "high"
        },
        {
          "rule_id": "R002",
          "description": "文档必须包含系统概述节，描述系统和软件的用途、一般特性，以及开发、运行和维护历史。",
          "check_type": "semantic",
          "keywords": [
            "系统概述",
            "用途",
            "一般特性",
            "历史"
          ],
          "positive_examples": [
            "系统概述节描述软件用于数据处理，特性包括高可靠性，开发始于2022年。"
          ],
          "negative_examples": [
            "系统概述节仅简单提及软件名称，未描述用途或历史。"
          ],
          "severity": "medium"
        },
        {
          "rule_id": "R003",
          "description": "文档必须包含文档概述节，概述文档的用途和内容。",
          "check_type": "semantic",
          "keywords": [
            "文档概述",
            "用途",
            "内容"
          ],
          "positive_examples": [
            "文档概述节说明本文档用于记录单元测试过程和结果。"
          ],
          "negative_examples": [
            "文档概述节缺失或内容空泛。"
          ],
          "severity": "low"
        }
      ]
    },
    {
      "category": "引用文档",
      "rules": [
        {
          "rule_id": "R004",
          "description": "引用文档节必须列出所有引用文档的编号、标题、版本和日期。",
          "check_type": "format",
          "keywords": [
            "引用文档",
            "编号",
            "标题",
            "版本",
            "日期"
          ],
          "positive_examples": [
            "引用文档：XX软件详细设计说明（标识），版本V2.0，日期2023-05-01。"
          ],
          "negative_examples": [
            "引用文档仅列出标题，缺少版本或日期信息。"
          ],
          "severity": "medium"
        }
      ]
    },
    {
      "category": "测试结果概述",
      "rules": [
        {
          "rule_id": "R005",
          "description": "测试结果概述必须包含对被测试软件的总体评估说明，并使用单元测试总体评估表（度量表）。",
          "check_type": "structure",
          "keywords": [
            "总体评估",
            "单元测试总体评估表",
            "度量表"
          ],
          "positive_examples": [
            "总体评估节包含表1，记录测试通过率、缺陷密度等度量数据。"
          ],
          "negative_examples": [
            "总体评估节仅文字描述，未使用指定表格格式。"
          ],
          "severity": "high"
        },
        {
          "rule_id": "R006",
          "description": "必须描述测试环境与实际环境的差异性，并评估其对测试结果的影响。",
          "check_type": "semantic",
          "keywords": [
            "测试环境",
            "差异性",
            "影响",
            "评估"
          ],
          "positive_examples": [
            "测试环境节说明硬件配置差异，并分析可能导致的性能偏差。"
          ],
          "negative_examples": [
            "测试环境节未提及任何差异或影响评估。"
          ],
          "severity": "medium"
        },
        {
          "rule_id": "R007",
          "description": "必须给出测试充分性指标数据。",
          "check_type": "semantic",
          "keywords": [
            "测试充分性",
            "指标数据"
          ],
          "positive_examples": [
            "测试充分性结果节提供代码覆盖率、路径覆盖率等具体数据。"
          ],
          "negative_examples": [
            "测试充分性结果节仅定性描述，无量化指标。"
          ],
          "severity": "high"
        },
        {
          "rule_id": "R008",
          "description": "必须提供改进建议，包括建议内容及其对软件的影响。",
          "check_type": "semantic",
          "keywords": [
            "改进建议",
            "建议",
            "影响"
          ],
          "positive_examples": [
            "改进建议节建议优化算法以提高效率，并分析可能减少10%处理时间。"
          ],
          "negative_examples": [
            "改进建议节仅列出问题，未提供具体建议或影响分析。"
          ],
          "severity": "medium"
        }
      ]
    },
    {
      "category": "静态检查记录",
      "rules": [
        {
          "rule_id": "R009",
          "description": "静态检查记录必须按Q/WE895（编码规则）及单元静态分析和代码审查要求进行，并在代码审查表中记录结果。",
          "check_type": "structure",
          "keywords": [
            "静态检查",
            "Q/WE895",
            "代码审查表",
            "记录"
          ],
          "positive_examples": [
            "静态检查记录节引用Q/WE895，并附代码审查表作为附件。"
          ],
          "negative_examples": [
            "静态检查记录节未提及Q/WE895或缺少代码审查记录。"
          ],
          "severity": "high"
        },
        {
          "rule_id": "R010",
          "description": "检查问题必须进行确认修改，如不需修改需软件项目负责人签字；修改后应进行回归检查并汇总结果。",
          "check_type": "semantic",
          "keywords": [
            "问题确认",
            "修改",
            "回归检查",
            "汇总"
          ],
          "positive_examples": [
            "问题处理记录显示所有问题已修改，回归检查后汇总在附件中。"
          ],
          "negative_examples": [
            "问题记录未说明是否修改或缺少负责人签字。"
          ],
          "severity": "medium"
        }
      ]
    },
    {
      "category": "单元测试记录",
      "rules": [
        {
          "rule_id": "R011",
          "description": "单元测试记录必须分小节或表逐一记录单元动态测试情况，可使用测试工具自动生成或参考表2格式。",
          "check_type": "format",
          "keywords": [
            "单元测试记录",
            "动态测试",
            "表2",
            "格式"
          ],
          "positive_examples": [
            "单元测试记录节使用表2格式，列出每个模块的测试用例和结果。"
          ],
          "negative_examples": [
            "单元测试记录节仅文字描述，未使用指定表格或分节记录。"
          ],
          "severity": "high"
        },
        {
          "rule_id": "R012",
          "description": "如合并单元集成测试记录，必须给出单元集成顺序及对应测试用例，参考表3格式。",
          "check_type": "structure",
          "keywords": [
            "单元集成测试",
            "集成顺序",
            "测试用例",
            "表3"
          ],
          "positive_examples": [
            "单元集成测试节使用表3，说明模块A->B的集成顺序和测试用例。"
          ],
          "negative_examples": [
            "合并测试记录但未提供集成顺序或测试用例。"
          ],
          "severity": "medium"
        }
      ]
    },
    {
      "category": "问题说明",
      "rules": [
        {
          "rule_id": "R013",
          "description": "问题说明必须对单元测试中发现的问题及其处理进行说明，可使用问题处理单（如表4）或从软件过程管理工具下载。",
          "check_type": "format",
          "keywords": [
            "问题说明",
            "问题处理单",
            "表4",
            "处理"
          ],
          "positive_examples": [
            "问题说明节使用表4格式，记录问题描述、处理状态和责任人。"
          ],
          "negative_examples": [
            "问题说明节仅简单列出问题，未说明处理过程或格式。"
          ],
          "severity": "medium"
        }
      ]
    },
    {
      "category": "需求追踪",
      "rules": [
        {
          "rule_id": "R014",
          "description": "需求追踪必须包含正向追溯和逆向追溯，分别使用表5和表6格式。",
          "check_type": "structure",
          "keywords": [
            "需求追踪",
            "正向追溯",
            "逆向追溯",
            "表5",
            "表6"
          ],
          "positive_examples": [
            "需求追踪节包含表5（需求到测试用例）和表6（测试用例到需求）。"
          ],
          "negative_examples": [
            "需求追踪节缺失或仅有一种追溯方式。"
          ],
          "severity": "high"
        }
      ]
    },
    {
      "category": "注释",
      "rules": [
        {
          "rule_id": "R015",
          "description": "注释节必须包括有助于了解文档的所有信息，如背景、术语、缩略语或公式。",
          "check_type": "semantic",
          "keywords": [
            "注释",
            "背景",
            "术语",
            "缩略语",
            "公式"
          ],
          "positive_examples": [
            "注释节定义术语如'单元测试'，列出缩略语如'API'。"
          ],
          "negative_examples": [
            "注释节内容空泛或缺失关键解释信息。"
          ],
          "severity": "low"
        }
      ]
    }
  ]
}